<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="User-Endpoint-Test-Suite">
    <test name="Util-Tests" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.carbon.identity.captcha.util.CaptchaUtilTest"/>
        </classes>
    </test>
    <test name="Captcha-Provider-Tests" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.carbon.identity.captcha.provider.mgt.CaptchaConfigServiceImplTest"/>
            <class name="org.wso2.carbon.identity.captcha.provider.mgt.CaptchaRuntimeServiceImplTest"/>
            <class name="org.wso2.carbon.identity.captcha.provider.mgt.provider.GoogleRecaptchaProviderTest"/>
            <class name="org.wso2.carbon.identity.captcha.provider.mgt.provider.GoogleRecaptchaEnterpriseProviderTest"/>
        </classes>
    </test>
    <test name="Captcha-Listener-Tests" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.carbon.identity.captcha.listener.CaptchaFlowExecutionListenerTest"/>
        </classes>
    </test>
</suite>
